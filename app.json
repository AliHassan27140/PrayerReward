{
  "expo": {
    "name": "Hayye",
    "slug": "hayye",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Icon.jpeg",
    "scheme": "hayye",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.name.sourceCode",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1",
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "versionCode": 1,
      "package": "org.name.sourceCode"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-splash-screen",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ef531fda-213f-4418-aecc-047dcbbfacc1"
      }
    }
  }
}
